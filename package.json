{
  "name": "express-template",
  "version": "0.0.1",
  "description": "Template for express.js",
  "author": "Han Moe Htet",
  "license": "MIT",
  "private": "true",
  "main": "src/index.js",
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "start": "nodemon --watch src --exec babel-node src/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "build": "rimraf build && babel src -d build",
    "serve": "cross-env NODE_ENV=production node build/index.js",
    "pm2": "pm2 start npm --name express-template -- run serve",
    "link": "node ./scripts/link.js",
    "lint:check": "eslint --cache ./",
    "lint:fix": "eslint --cache --fix ./",
    "prettier:check": "prettier --check ./",
    "prettier:fix": "prettier --write ./",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "i18next": "^21.6.13",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "luxon": "^2.3.1",
    "pm2": "^5.2.0",
    "rate-limiter-flexible": "^2.3.6",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache",
    "*.{js,md}": "prettier --check"
  }
}
